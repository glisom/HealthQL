{
  "name": "react-native-healthql",
  "version": "1.2.1",
  "description": "SQL-like query language for Apple HealthKit in React Native",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc && tsc -p plugin/tsconfig.json",
    "clean": "rm -rf build plugin/build coverage",
    "prepare": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit"
  },
  "expo": {
    "autolinking": {
      "nativeModulesDir": "."
    }
  },
  "keywords": [
    "react-native",
    "expo",
    "healthkit",
    "health",
    "sql",
    "ios"
  ],
  "author": "Grant Isom <glisom@icloud.com>",
  "license": "MIT",
  "homepage": "https://github.com/glisom/HealthQL#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/glisom/HealthQL.git",
    "directory": "packages/react-native-healthql"
  },
  "bugs": {
    "url": "https://github.com/glisom/HealthQL/issues"
  },
  "peerDependencies": {
    "expo": ">=49.0.0",
    "expo-modules-core": ">=1.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.72.0"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "expo": "^52.0.0",
    "expo-modules-core": "^1.12.0",
    "jest": "^29.7.0",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
